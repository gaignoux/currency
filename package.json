{
  "name": "@gaignoux/currency",
  "version": "0.1.3",
  "description": "A way to get all currency information from around the world.",
  "packageScope": "@gaignoux",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "private": false,
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@open-wc/building-rollup": "^3.0.2",
    "@types/node": "^20.12.12",
    "@types/shelljs": "^0.8.15",
    "deepmerge": "^4.3.1",
    "es-dev-server": "^2.1.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.7",
    "rollup": "^4.17.2",
    "shelljs": "^0.8.5",
    "terser-webpack-plugin": "^5.3.10",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths-jest": "^0.0.1",
    "typedoc": "^0.25.13",
    "typescript": "^5.4.5"
  },
  "scripts": {
    "start": "npx webpack serve",
    "prebuild": "rimraf dist",
    "build": "tsc && rollup -c rollup.config.mjs",
    "build:docs": "npx typedoc --entryPoints src/enums/index.ts --entryPoints src/types/index.ts --entryPoints src/constants/index.ts --entryPoints src/utils/index.ts",
    "publish:docs": "ts-node --esm tools/gh-docs-publish.ts",
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watch",
    "format": "npx prettier . --write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gaignoux/currency.git"
  },
  "keywords": [
    "currency",
    "typescript",
    "currency-symbol",
    "currency-code"
  ],
  "author": "Bruno Gaignoux",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/gaignoux/currency/issues"
  },
  "homepage": "https://gaignoux.github.io/currency"
}
